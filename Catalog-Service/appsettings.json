{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Consul": {
    "Address": "http://consul-client:8500",
    "ServiceName": "CatalogService",
    "ServiceId": "CatalogService-1",
    "ServiceAddress": "catalog-service",
    "UseDockerHostname": true,
    "ServicePort": 5000,
    "ServiceTags": [
      "catalog",
      "dotnet",
      "v1"
    ],
    "CheckInterval": 10,
    "DeregisterCriticalServiceAfterMinutes": 1
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:5000"
      }
    }
  }
}