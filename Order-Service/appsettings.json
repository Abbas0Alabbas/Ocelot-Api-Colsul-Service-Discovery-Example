{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Consul": {
    "Address": "http://consul-client:8500",
    "ServiceName": "OrderService",
    "ServiceId": "OrderService-1",
    "ServiceAddress": "order-service",
    "UseDockerHostname": true,
    "ServicePort": 5001,
    "ServiceTags": [
      "order",
      "dotnet",
      "v1"
    ],
    "CheckInterval": 10,
    "DeregisterCriticalServiceAfterMinutes": 1
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:5001"
      }
    }
  }
}